

<IfModule mod_expires.c>
ExpiresActive On
ExpiresByType image/jpg "access 30 year"
ExpiresByType image/jpeg "access 30 year"
ExpiresByType image/gif "access 30 year"
ExpiresByType image/png "access 30 year"
ExpiresByType image/webp "access 30 year"
ExpiresByType image/svg+xml "access 30 year"
ExpiresByType text/css "access 30 year"
ExpiresByType application/pdf "access 30 year"
ExpiresByType application/javascript "access 30 year"
ExpiresByType application/x-javascript "access 30 year"
ExpiresByType application/x-shockwave-flash "access 30 year"
ExpiresByType image/x-icon "access 1 year"
ExpiresDefault "access 2 days"
</IfModule>

